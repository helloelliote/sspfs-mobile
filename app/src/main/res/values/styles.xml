<resources>

    <!--Components-->

    <style name="Widget.Sspfs.NavigationView" parent="Widget.Material3.NavigationView">
        <!--Remove any scrim insets applied by NavigationView for system bars-->
        <item name="insetForeground">@android:color/transparent</item>
        <!--Set the background color of NavigationView-->
        <item name="android:background">@android:color/transparent</item>
        <item name="elevation">0dp</item>
        <item name="itemTextAppearance">?attr/textAppearanceBody2</item>
    </style>

    <style name="App.LinearLayout.Horizontal" parent="Theme.AppTheme">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:baselineAligned">false</item>
        <item name="android:orientation">horizontal</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
    </style>

    <style name="App.TextInputLayout" parent="Widget.Material3.TextInputLayout.OutlinedBox.Dense">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginHorizontal">4dp</item>
        <item name="android:layout_marginVertical">4dp</item>
        <item name="android:textSize">18sp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="endIconMode">clear_text</item>
        <item name="helperTextEnabled">true</item>
    </style>

    <style name="App.TextInputEditText" parent="Widget.Material3.TextInputEditText.OutlinedBox.Dense">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:inputType">textNoSuggestions</item>
        <item name="maxLines">1</item>
        <item name="singleLine">true</item>
    </style>

    <style name="App.TextInputLayout.Disabled" parent="Widget.Material3.TextInputLayout.OutlinedBox.Dense">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginHorizontal">4dp</item>
        <item name="android:layout_marginVertical">4dp</item>
        <item name="android:textSize">18sp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="endIconMode">none</item>
        <item name="helperTextEnabled">true</item>
        <item name="boxBackgroundColor">@color/grey_200</item>
        <item name="helperText">@string/helper_text_autocomplete</item>
    </style>

    <style name="App.TextInputEditText.Disabled" parent="Widget.Material3.TextInputEditText.OutlinedBox.Dense">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:inputType">textNoSuggestions</item>
        <item name="android:focusable">false</item>
    </style>

    <style name="App.TextInputLayout.Dropdown" parent="Widget.Material3.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginHorizontal">4dp</item>
        <item name="android:layout_marginVertical">4dp</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
    </style>

    <style name="App.MaterialButton" parent="Widget.Material3.Button">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginHorizontal">4dp</item>
    </style>

    <style name="App.TextView.Legend" parent="Theme.Material3.Light.NoActionBar">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:drawableRight">@drawable/ic_round_location_on_36</item>
    </style>

    <style name="App.TableLayout.TextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingVertical">18dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:text">""</item>
        <item name="android:background">@drawable/tablelayout_border_row</item>
        <item name="android:layout_weight">1</item>
        <item name="android:focusable">false</item>
        <item name="android:clickable">false</item>
    </style>

    <style name="App.TableLayout.TextView.Small" parent="Widget.MaterialComponents.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingVertical">18dp</item>
        <item name="android:paddingHorizontal">2dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:text">""</item>
        <item name="android:background">@drawable/tablelayout_border_row</item>
        <item name="android:layout_weight">1</item>
        <item name="android:focusable">false</item>
        <item name="android:clickable">false</item>
    </style>

    <style name="App.TableLayout.TextView.Select" parent="Widget.MaterialComponents.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingVertical">18dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:text">""</item>
        <item name="android:background">@drawable/tablelayout_border_row_deselect</item>
        <item name="android:layout_weight">1</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="App.TableLayout.TextView.Small.Select" parent="Widget.MaterialComponents.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingVertical">18dp</item>
        <item name="android:paddingHorizontal">2dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:text">""</item>
        <item name="android:background">@drawable/tablelayout_border_row_deselect</item>
        <item name="android:layout_weight">1</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="App.TableLayout.Label" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingVertical">16dp</item>
        <item name="android:paddingEnd">0dp</item>
        <item name="android:paddingStart">1dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:text">""</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:backgroundTint">#f5f5f5</item>
        <item name="android:layout_weight">1</item>
        <item name="android:focusable">false</item>
        <item name="android:clickable">false</item>
        <item name="cornerRadius">0dp</item>
        <item name="strokeColor">#d1d1d1</item>
        <item name="strokeWidth">0.1dp</item>
    </style>

    <style name="App.TableLayout.EditText" parent="Theme.AppTheme">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingHorizontal">5dp</item>
        <item name="android:paddingVertical">18dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/tablelayout_border_row_edittext</item>
        <item name="android:layout_weight">1</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:maxLines">1</item>
        <item name="android:inputType">text</item>
    </style>

    <style name="App.TableLayout.ImageView" parent="Theme.AppTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">275dp</item>
        <item name="android:scaleType">center</item>
        <item name="srcCompat">@drawable/ic_round_image_search_96</item>
        <item name="android:background">@drawable/tablelayout_border</item>
        <item name="contentDescription">@null</item>
    </style>

    <style name="App.TableLayout.ImageView.Button" parent="Widget.Material3.Button.TextButton.Icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="App.TableLayout.ImageView.TextView" parent="Theme.AppTheme">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="MaterialAlertDialog__Center" parent="@style/ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="materialAlertDialogTitlePanelStyle">@style/Title.Panel.Center</item>
        <item name="materialAlertDialogBodyTextStyle">@style/Message.Center</item>
    </style>

    <style name="Title.Panel.Center" parent="MaterialAlertDialog.MaterialComponents.Title.Panel">
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="Message.Center" parent="MaterialAlertDialog.MaterialComponents.Body.Text">
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="CustomButtonStyle" parent="Widget.Material3.Button.TonalButton">
        <item name="android:textSize">16sp</item>
        <item name="cornerRadius">6dp</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_gravity">fill_horizontal</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_columnWeight">1</item>
        <item name="android:layout_weight">0.25</item>
    </style>
</resources>
